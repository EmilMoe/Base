{
    "name": "emilmoe/base",
    "type": "extension",
    "description": "Base structure files for Laravel such as a base view.",
    "license": "proprietary",
    "authors": [{
        "name": "Emil Moe",
        "email": "emil@moegroup.dk",
        "homepage": "https://moegroup.dk"
    }],
    "require": {
        "illuminate/support": "~5.7",
        "php": "~7.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0"
    },
    "autoload": {
        "psr-4": {
            "EmilMoe\\Base\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "EmilMoe\\Base\\BaseServiceProvider"
            ]
        },
        "emilmoe": {
            "autoloader": [
            ]
        }
    }
}